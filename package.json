{
  "name": "express-remix-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "build": "remix vite:build",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "prod": "npm run build && npm run start",
    "dev": "node ./server.js",
    "typecheck": "tsc",
    "clean": "rm -rf .vite_cache .cache dist build",
    "clean-deep": "rm -rf node_modules package-lock.json .vite_cache .cache dist build",
    "clean-install": "npm run clean-deep && npm install",
    "clean-dev": "npm run clean && npm run dev",
    "clean-start": "npm run clean && npm run prod"
  },
  "dependencies": {
    "@remix-run/express": "^2.9.2",
    "@remix-run/node": "^2.9.2",
    "@remix-run/react": "^2.9.2",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "isbot": "^4.1.0",
    "morgan": "^1.10.0",
    "react": "18.3.0-canary-a870b2d54-20240314",
    "react-dom": "18.3.0-canary-a870b2d54-20240314"
  },
  "overrides": {
    "react": "18.3.0-canary-a870b2d54-20240314",
    "react-dom": "18.3.0-canary-a870b2d54-20240314"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.9.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
